{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "lto-data",
      "host": {
        "sourcePath": "/lto/data"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "public-node",
      "image": "legalthings/public-node",
      "essential": true,
      "memory": 1984,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 6869
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "lto-data",
          "containerPath": "/lto/data"
        }
      ]
    }
  ]
}
